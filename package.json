{
  "name": "stv",
  "version": "0.0.0",
  "description": "STV vote code repository for the Star Atlas DAO",
  "scripts": {
    "build": "pnpm run -r build",
    "watch": "pnpm build && pnpm run -r --parallel watch",
    "pretest": "pnpm build",
    "test": "jest --verbose",
    "clean": "pnpm run -r --parallel clean && rimraf -g node_modules/*",
